<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Doctor List - Clinic B</title>
  <style>
    table {
      width: 100%;
      border-collapse: collapse;
      margin-top: 20px;
    }
    th, td {
      padding: 8px;
      text-align: left;
      border-bottom: 1px solid #ddd;
    }
    th {
      background-color: #007bff;
      color: white;
    }
  </style>
</head>
<body>
  <h1>Doctor List - Clinic B</h1>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Specialization</th>
        <th>About</th>
        <th>SLMC Number</th>
        <th>Clinic</th>
        <th>Image</th>
      </tr>
    </thead>
    <tbody>
      <% doctors.forEach(doctor => { %>
        <tr>
          <td><%= doctor.name %></td>
          <td><%= doctor.specialization %></td>
          <td><%= doctor.about %></td>
          <td><%= doctor.slmcNumber %></td>
          <td><%= doctor.clinic %></td>
          <td><img src="<%= doctor.image %>" alt="<%= doctor.name %>" style="width: 100px;"></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
