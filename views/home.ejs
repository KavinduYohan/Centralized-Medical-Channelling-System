<section id="clinic" class="p-80px-tb bg-light">
   <div class="container">
      <div class="row">
         <div class="col-sm-8 offset-sm-2">
            <div class="section-title text-center m-60px-b">
               <h2>Our Clinics</h2>
               <div class="divider-circle mx-auto"></div>
               <p>Explore our network of clinics where you can receive personalized care.</p>
            </div>
         </div>
      </div>
            <div class="row">
               <% if (clinics && clinics.length) { %>
                  <% clinics.forEach(clinic => { %>
                     <div class="col-md-3 col-sm-6 sm-m-15px-tb text-center">
                        <a href="/clinic/<%= clinic._id %>" class="clinic-item-link">

                           <div class="clinic-item">
                              <img src="<%= clinic.image || 'https://www.centralmedicaltt.com/assets/img/slider/slider-1.jpg' %>" alt="Clinic Image" class="clinic-image">
                              <h4><%= clinic.clinicName %></h4>
                              
                           </div>
                        </a>
                     </div>
                  <% }) %>
               <% } else { %>
                  <p>No clinics found.</p>
               <% } %>
            </div>
         </div>
          
         <!-- Repeat for additional clinics -->
      </div>
   </div>
</section>
<section id="all-doctors" class="p-80px-tb bg-light">
